{
  "manifest_version": 2,
  "name": "WebNotes",
  "version": "1.0",
  "description": "This is a test description change it later!",
  "icons": {
    "16": "/icons/robit_16.png",
    "48": "/icons/robit_48.png",
    "128": "/icons/robit_128.png"
  },
  "background": {
   // "scripts": ["/background/background.js"]
   "page": "/background/background.html"
 },
  "permissions": ["tabs",
  "contextMenus",
  "activeTab",
  "background",
  "*://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "/icons/robit_sleep_19.png",
      "38": "/icons/robit_48.png"
    },
 "content_scripts": [{
    "js": ["/content/content.js"],
    "matches": [ "*://*/*"]
  }],
    "default_title": "WebNotes",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "/js/dependencies/externalMain.js"
  ]
}

